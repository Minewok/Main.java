+public class HubContenedores extends JFrame{
+
+    public JPanel mainPanel;
+    protected JLabel lblNumIden;
+    protected JTextField txtNumIden;
+    protected JLabel lblPeso;
+    protected JTextField txtPeso;
+    protected JLabel lblDesc;
+    protected JTextArea txtareaDesc;
+    protected JLabel lblEnv;
+    protected JTextField textField1;
+    protected JLabel lblRec;
+    protected JTextField textField2;
+    protected JLabel lblPais;
+    protected JComboBox ComBoxPais;
+    protected JLabel lblPrioridad;
+    protected JRadioButton rbtnPrior1;
+    protected JRadioButton rbtnPrior2;
+    protected JRadioButton rbtnPrior3;
+    protected JCheckBox checkBoxInspeccion;
+    protected JLabel lblEstado;
+    protected JTextArea txtareaHub;
+    protected JPanel Operaciones;
+    protected JButton btnApilar;
+    protected JButton btnDesapilar;
+    protected JTextField txtNumColumna;
+    protected JButton btnDatos;
+    protected JTextField txtIdContenedor;
+    protected JButton btnCuantos;
+    protected JComboBox comboBoxPais2;
+    protected JTextField txtCantidad;
+    protected JLabel lblImagen;
+
+    private Contenedores[][] matriz;
+
+    HubContenedores() {
+        matriz = new Contenedores[10][12];
+        for (int c = 0; c <= 10; c++) {
+            for (int f = 0; f <= 12; f++) {
+                matriz[c][f] = new Contenedores(0, 0, 0, "0", "0", "0", "0", false);
+            }
+        }
+    }
+
+    public String toString() {
+        //Random random = new Random();
+        for (int i = 0; i < matriz.length; i++) {//numero de filas
+            for (int j = 0; j < matriz[i].length; j++) {//numero de columnas
+                //matriz[i][j] = random.nextInt(10);
+                System.out.print(matriz[i][j] + " ");
+            }
+            System.out.println();
+        }
+        return Integer.toString(matriz[][]);
+    }
+
+    public void retirarContenedor(int NumeroColumna){
+        int fila=0;
+        for(int f = 0; f < 12; f++) {
+            if((matriz[NumeroColumna][f].getId()!=0) && (fila=0)) {
+                fila=f;
+            }
+        }
+        matriz[NumeroColumna][fila].setId(0);
+    }
+
+    public Contenedores apilarContenedor() {
+        String Descripcion = this.txtareaDesc.getText();
+        String Pais = (String) this.comboBoxPais2.getSelectedItem();
+        String EmpresaReceptora = this.textField2.getText();
+        String EmpresaSaliente = this.textField1.getText();
+        Boolean Inspeccion = this.checkBoxInspeccion.isSelected();
+        int Prioridad = 0;
+        int id = Integer.parseInt(this.txtNumIden.getText());
+        int Peso = Integer.parseInt(this.txtPeso.getText());
+        if (this.rbtnPrior1.isSelected()==true){
+            Prioridad = 1;
+        }else if(rbtnPrior2.isSelected()==true){
+            Prioridad = 2;
+        }else if(rbtnPrior3.isSelected()==true){
+            Prioridad = 3;
+        }
+        return new Contenedores(id,Peso,Prioridad,Descripcion,Pais,EmpresaSaliente,EmpresaReceptora,Inspeccion);
+
+
+    }
+
+    public void mostrarInfo() {
+
+        int IdRevisada = Integer.parseInt(this.txtIdContenedor.getText());
+
+        for(int c = 0; c <= 10; c++) {
+            for(int f = 0; f <= 12; f++) {
+                if(matriz[c][f].getId() == IdRevisada){
+                    System.out.println("Id: "+matriz[c][f].getId());
+                    System.out.println("Peso: "+matriz[c][f].getPeso());
+                    System.out.println("Prioridad: "+matriz[c][f].getPrioridad());
+                    System.out.println("Descripción: "+matriz[c][f].getDescripcion());
+                    System.out.println("País: "+matriz[c][f].getPais());
+                    System.out.println("Emisor: "+matriz[c][f].getEnvio());
+                    System.out.println("Destinatario: "+matriz[c][f].getRecibo());
+                    System.out.println("Inspección: "+matriz[c][f].getInspeccion());
+                }
+            }
+        }
+    }
+
+
+
+
