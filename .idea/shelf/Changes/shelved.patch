+package PaqG09;
+
+public class Contenedores extends HubContenedores {
+
+    private int idContenedor, pesoContenedor, nivelPrioridad;
+    private String descripcion, paisProcedencia, empresaEnvio, empresaRecibo;
+    private boolean inspeccion;
+
+    public Contenedores(int idContenedor, int pesoContenedor, int nivelPrioridad, String descripcion,
+                        String paisProcedencia, String empresaEnvio, String empresaRecibo, boolean inspeccion) {
+
+        if (descripcion.length() <= 100) {
+            this.descripcion = descripcion;
+        }
+        else System.out.println("La descripcion es demasiado larga.");
+
+        if (empresaEnvio.length() <= 20 || empresaRecibo.length() <= 20) {
+            this.empresaEnvio = empresaEnvio;
+            this.empresaRecibo = empresaRecibo;
+        }
+        else System.out.println("El nombre de empresa no puede superar los 20 caracteres.");
+
+        this.paisProcedencia = paisProcedencia;
+        this.inspeccion = inspeccion;
+        this.idContenedor = idContenedor;
+        this.pesoContenedor = pesoContenedor;
+        this.nivelPrioridad = nivelPrioridad;
+
+    }
+
+    //Getters
+    public String getDescripcion( ) { return descripcion; }
+    public String getPais() { return paisProcedencia; }
+    public String getEnvio() { return empresaEnvio; }
+    public String getRecibo() { return empresaRecibo; }
+
+    public int getId() { return idContenedor; }
+    public int getPeso() { return pesoContenedor; }
+    public int getPrioridad() { return nivelPrioridad; }
+
+    public boolean getInspeccion() { return inspeccion; }
+
+    //Setters
+    public void setDescripcion(String descripcion) {
+        if (descripcion.length() <= 100) this.descripcion = descripcion;
+        else System.out.println("La descripcion es demasiado larga.");
+    }
+    public void setPais(String paisProcedencia) { this.paisProcedencia = paisProcedencia; }
+    public void setEnvio(String empresaEnvio) {
+        if (empresaEnvio.length() <= 20) this.empresaEnvio = empresaEnvio;
+        else System.out.println("El nombre de la empresa que envia el paquete es demasiado largo.");
+    }
+    public void setRecibo(String empresaRecibo) {
+        if (empresaRecibo.length() <= 20) this.empresaRecibo = empresaRecibo;
+        else System.out.println("El nombre de la empresa que recibe el paquete es demasiado largo.");
+    }
+    public void setId(int idContenedor) { this.idContenedor = idContenedor; }
+    public void setPeso(int pesoContenedor) { this.pesoContenedor = pesoContenedor; }
+    public void setPrioridad(int nivelPrioridad) { this.nivelPrioridad = nivelPrioridad; }
+
+    public void setInspeccion(boolean inspeccion) { this.inspeccion = inspeccion; }
+
+}
